    AND
    |OR
    |NEGACION
;
typevariable::=
    CARACTERESVARIABLES MAS typevariable
    |CADCOM MAS typevariable
    |CADCOMSIM MAS typevariable
    |DECIMAL MAS typevariable
    |NUMBER MAS typevariable
    
    |CARACTERESVARIABLES
    |CADCOM
    |CADCOMSIM
    |DECIMAL
    |NUMBER
;

